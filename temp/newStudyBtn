	newSleepStudyBtn->clicked().connect(std::bind([=]() {
		//new study id

		Wt::WDialog *newstdialog = new Wt::WDialog("New Sleep Study");
		Wt::WContainerWidget *newstcontainer = new Wt::WContainerWidget(newstdialog->contents());
		
		Wt::WText *hellonewst = new Wt::WText("Hello and welcome to a new sleep study");
		newstcontainer->addWidget(hellonewst);
		
		staticStd_ = new Wt::WText();
		staticStd_->setText(setStdId());
		
		composer_ = new StudyComposer(staticStd_->text(), this);

		newstcontainer->addWidget(new Wt::WText("<h4>Upload Referral</h4>"));
		newstcontainer->addWidget(new Wt::WText("Upload the Referral or Order PDF or image from the efax system!"));
		newstcontainer->addWidget(composer_);
		newstcontainer->show();

		Wt::WPushButton *ok = new Wt::WPushButton("Next", newstdialog->footer());
		ok->setDefault(true);


		Wt::WPushButton *cancel = new Wt::WPushButton("Cancel", newstdialog->footer());
		newstdialog->rejectWhenEscapePressed();

		ok->clicked().connect(std::bind([=]() {
			newstdialog->accept();
			//
		}
		cancel->clicked().connect(newstdialog, &Wt::WDialog::reject);
		
		newstdialog->show();

	}));